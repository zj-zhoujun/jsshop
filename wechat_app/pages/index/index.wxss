/**index.wxss**/
page{
  background-color:#f3f3f3;
}
.search-input{
  background-color: #fff;
  position:relative;
}
swiper{
  height: 376rpx;
  background-color: #fff;
}
swiper-item{
  width: 750rpx;
  height: 376rpx;
}
.slide-image{
  width: 100%;
  height: 100%; 
}
.news-coupons-mid{
  margin: 16rpx 0;
  width: 690rpx;
  padding: 0 30rpx 16rpx;
  background-color: #fff;
}
.news{
  font-size: 26rpx;
  color: #888;
  padding-top: 16rpx;
}
.news image{
  width: 28rpx;
  height: 26rpx;
  margin-right: 16rpx;
}
.swiper_news{
    height: 34rpx;
    width: 86vw;
    display: inline-block;
}  
.swiper_item {
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  letter-spacing: 2px;
}
.coupons{
  overflow: hidden;
}
.coupons view{
  width: 220rpx;
  height: 120rpx;
  line-height: 120rpx;
  float: left;
  text-align: center;
  margin-top: 16rpx;
  font-size: 24rpx;
  margin-right: 14rpx;
  box-sizing: border-box;
}
.coupons view:nth-child(3n){
  margin-right: 0;
}
.coupons>view:nth-child(n){
  color: rgb(249, 164, 203);
  background-color: rgb(255, 245, 244);
  border-left: 2rpx dashed rgb(249, 164, 203);
}
.coupons>view:nth-child(2n){
  color: rgb(253, 179, 58);
  background-color: rgb(253, 244, 237);
  border-left: 2rpx dashed rgb(253, 179, 58);
}
.coupons>view:nth-child(3n){
  color: rgb(244, 144, 155);
  background-color: rgb(255, 241, 241);
  border-left: 2rpx dashed rgb(244, 144, 155);
}
.button-list{
  /* border-bottom: 2rpx solid #f3f3f3; */
  background-color: #fff;
  margin: 16rpx 0;
  padding: 32rpx 45rpx;
}
.button-list .button-list-mid{
  width: 660rpx;
  margin: 0 auto;
  overflow: hidden;
}
.button-list .button-list-mid .nav{
  width: 20%;
  float: left;
  text-align: center;
  font-size: 24rpx;
  color: #666;
  /* margin-bottom: 32rpx; */
}
.button-list .button-list-mid .nav image{
  width: 94rpx;
  height: 94rpx;
  border-radius: 90rpx;
}
.button-list .button-list-mid .nav text{
  display: block;
}
.seller-recommends, .hot-recommends, .group-recommends{
  text-align: center;
  /* background-color: #f9f9f9; */
}

.seller-recommends-mid,.hot-recommends-mid{
  width: 700rpx;
  margin: 0 auto;
}
.recommends-img{
  width: 24rpx;
  height: 24rpx;
  margin-right: 6rpx;
}
.recommends-header{
  font-size: 32rpx;
  color: #666;  
  margin: 0 0 20rpx;
  padding-top: 20rpx;
}
.seller-recommends-body, .hot-recommends-body, .group-recommends-body{
  overflow: hidden;
}
.seller-recommends-body .seller-body-mid{
  width: 242rpx;
  float: left;
   text-align: center; 
  font-size: 24rpx;
  color: #666;
  margin-bottom: 12rpx; 
  margin-right: 12rpx;
  box-sizing: border-box;
}
 .seller-recommends-body>view:nth-child(3n+0){
  background-color: #f9f9f9;
} 
.seller-recommends-body>view:nth-child(3n+0) .seller-body-mid{
  margin-right: 0;
}
.seller-recommends-body .seller-body-mid .pic{
  width: 242rpx;
  height: 242rpx;
  box-sizing: border-box;
}
.recommends-body-bottom{
  border-top: none;
  box-sizing: border-box;
  padding: 10rpx 6rpx;
  margin: -10rpx auto 0;
  position: relative;
  text-align: left;
  background-color: #fff
}
.price-cart{
  text-align: left;
  color: #FF3A59;
  font-size: 32rpx;
  width: 220rpx;
  overflow: hidden;
  margin-top: 10rpx;
}
.price-time{
  font-size: 24rpx;
  margin-bottom: 6rpx;
  color: #888;
}
.price-time>text{
  display: inline-block;
  padding: 0 6rpx;
  border-radius: 6rpx;
  background-color: rgb(255, 194, 203);
  color: #FF3A59;
  font-size: 22rpx;
}
.price-btn{
  background-color: #FF3A59;
  color: #fff;
  font-size: 24rpx;
  display: inline-block;
  float: right;
  padding: 4rpx 10rpx;
  border-radius: 10rpx;
}
.hot-recommends-body .price-cart{
  width: 340rpx;
}
.seller-recommends .recommends-body-bottom{  
  width: 242rpx; 
  height: 150rpx;  
}
.seller-recommends .name{
  width: 232rpx;
}
hot-recommends .name{
  width: 356rpx;
}
.recommends-body-bottom image.icon{
  width: 44rpx;
  height: 44rpx;
  float: right;
}
.hot-recommends-body image.icon{
  width: 56rpx;
  height: 56rpx;
}
.hot-recommends-body .hot-body-mid{
  width: 366rpx;
  float: left;
  margin-bottom: 18rpx;
  font-size: 26rpx;
  color: #666;
  margin-right: 18rpx;
}
.hot-recommends-body .hot-body-mid:nth-child(2n+0){
  margin-right: 0
}
.hot-body-mid .pic{
  width: 366rpx;
  height: 366rpx;
  box-sizing: border-box;
}
.hot-recommends .recommends-body-bottom{
  width: 366rpx;
  height: 160rpx;
}
.group-recommends .recommends-body-bottom{
  width: 366rpx;
  /* height: 190rpx; */
}
.name{
  overflow:hidden;
  text-overflow: ellipsis;  
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
  white-space: normal;
  min-height:76rpx;

}
.loading{
  font-size:22rpx;
  color:#bbb;
}

.coupon-red{
  width: 694rpx;
  margin: 30rpx auto 0;
  border-radius: 10rpx;
  overflow: hidden;
  position: relative;
  background-color: #fff;
  box-sizing: border-box;
}
.coupon-bg{
  position: absolute;
  width: 100%;
  height: 160rpx;
}
.coupon-c{
  position: relative;
  z-index: 100;
  width: 100%;
  height: 160rpx;
  box-sizing: border-box;
  background: linear-gradient(to right, #ff6565 , #ff8a8a);
}
.red-left,.gray-left{
  width: 28%;
  height: 100rpx;
  color: rgb(255, 247, 247);
  font-size: 22rpx;
  padding: 0 30rpx;
  float: left;
  margin: 30rpx 0;
  text-align: center;
  box-sizing: border-box;
  border-right: 2rpx dotted #fff;
  display:inline-block;
}
.red-right,.gray-right{
  width: 50%;
  height: 100%;
  font-size: 22rpx;
  float: left;
  text-align: left;
  color: #fff;
  box-sizing: border-box;
  padding: 20rpx 16rpx;
  display:inline-block;
}
.coupon-sum{
  max-height: 100%;
  font-size: 26rpx;
  line-height: 1.3;
  position: relative;
  top: 50%;
  transform: translateY(-50%);
  overflow: hidden;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:3;
  text-overflow: ellipsis;  
}
.red-right text,.gray-right text{
  display: block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.conpon-f{
  font-size: 28rpx;
}
.coupon-btn{
  padding: 0 16rpx;
  background-color: #ffe7e7;
  color: #ff5454;
  font-size: 24rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 30rpx;
  border-radius: 4rpx;
}
.conpon-bot{
  padding: 10rpx 30rpx;
  color: #999;
  font-size: 20rpx;
  border: 2rpx solid #f3f3f3;
  border-top: none;
}
.nav-down{
  height:188rpx;
}
.middle-advert{
  margin-top: 10rpx;
  overflow: hidden;
}
.middle-item{
  float: left;
  width:49%;
}
.middle-item.right{
  float: right;
}
.middle-item .left-item {
  width: 100%;
  height:376rpx;
}
.middle-item .swiper-item {
  width:100%;
  height:184rpx;
}
.middle-item .swiper-item.top{
  margin-bottom:8rpx;
}
.middle-item.item-left{
  margin-right:2%;
}
.middle-pic{
  width:100%;
  height:188rpx;
}
.every-commodity{
  padding: 20rpx 30rpx;  
  margin-bottom: 2rpx;
  overflow: hidden;
  background-color: #fff;
}
.commodity-pic{
  width: 240rpx;
  height: 240rpx;
  border-radius: 6rpx;
}
.every-commodity-right{
  width: 430rpx;
  display: inline-block;
  margin-left: 20rpx;
  /* height: 240rpx; */
  float: right;
  position: relative;
}
.commodity-name{
  font-size: 26rpx;
  color: #666;
  width: 430rpx;
  overflow:hidden;
  text-overflow: ellipsis;  
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
  white-space: normal;
}
.preferential{
    /* height: 48rpx; */
    /* overflow: hidden; */
    line-height:36rpx;
}
.preferential>text{
  display: inline-block;
  background-color: #FF3B44;
  color: #fff;
  font-size: 18rpx;
  margin-right: 18rpx;
  border-radius: 10rpx;
  height: 28rpx;
  line-height: 28rpx;
  padding:0 10rpx;
}
.price-cart{
  width: 420rpx;
  overflow: hidden;
  height:52rpx;
}
.price{
  font-size: 32rpx;
  color: #FF3B44;
}
.cart-img{
  width: 52rpx;
  height: 52rpx;
  float: right;
}
.loading{
  text-align: center;
  font-size: 28rpx;
  color: #999;
  margin: 10rpx 0;
  padding-bottom: 10rpx;
}
.lable_green{
  background-color: #009688 !important;
}
.lable_orange{
  background-color: #FFB800!important;
}
.lable_blue{
  background-color: #1E9FFF!important
}
.promotion_off{
  background-color:#ccc !important;
  color:#fff;
  font-size: 18rpx;
  border-radius: 10rpx;
  height:28rpx;
  line-height:28rpx;
  padding: 0 10rpx;
}
.none-img{
  margin: 100rpx auto;
  text-align: center;
}
.none-img image{
  width: 230rpx;
  height: 230rpx;
}
.none-img text{
  display: block;
  color:#ccc;
  font-size:28rpx;
}
.every-commodity-grid{
  width: 345rpx;
  float: left;
  box-sizing: border-box;
  border-bottom: 0;
  /* text-align: center; */
  padding: 0rpx;
  margin: 15rpx;
}
.every-commodity-grid:nth-child(even){
  margin-right: 15rpx;
  margin-left: 0;
}
.every-commodity-grid:nth-child(odd){
  margin-right: 30rpx;
}
.every-commodity-grid>image{
  width: 345rpx;
  height: 345rpx;
}
.every-commodity-grid .every-commodity-right{
  width: 100%;
  height: 236rpx;
  text-align: left;
  box-sizing: border-box;
  padding: 0rpx 20rpx;
}
.every-commodity-grid .commodity-name{
  width: 100%;
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-box-orient:vertical;
